{
  "name": "thachai-multi-platform-assistant",
  "version": "1.0.0",
  "description": "Multi-platform AI assistant supporting Google Assistant, Alexa, Microsoft Bot Framework, and custom integrations",
  "main": "platform_manager.js",
  "scripts": {
    "start": "node platform_manager.js",
    "dev": "nodemon platform_manager.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "webpack --mode production",
    "deploy": "pm2 start ecosystem.config.js",
    "google-assistant": "node -e \"require('./google_assistant_integration.py')\"",
    "alexa": "node -e \"require('./alexa_integration.py')\"",
    "microsoft-bot": "node microsoft_bot_framework.js",
    "custom-platform": "python custom_platform_integration.py",
    "start-all": "concurrently \"npm run start\" \"python google_assistant_integration.py\" \"python alexa_integration.py\" \"python custom_platform_integration.py\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "botbuilder": "^4.20.0",
    "botbuilder-ai": "^4.20.0",
    "restify": "^11.1.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "node-telegram-bot-api": "^0.64.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.0",
    "google-auth-library": "^9.4.0",
    "googleapis": "^128.0.0",
    "ask-sdk-core": "^2.14.0",
    "ask-sdk-model": "^1.82.0",
    "aws-sdk": "^2.1498.0",
    "azure-cognitiveservices-luis-runtime": "^5.0.0",
    "dialogflow": "^2.0.0",
    "grpc": "^1.24.11",
    "protobufjs": "^7.2.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "joi": "^17.11.0",
    "redis": "^4.6.10",
    "mongodb": "^6.3.0",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "mongoose": "^8.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "concurrently": "^8.2.2",
    "pm2": "^5.3.0",
    "@types/node": "^20.9.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "google-assistant",
    "alexa",
    "microsoft-bot",
    "voice",
    "chatbot",
    "nlp",
    "vietnam",
    "multi-platform"
  ],
  "author": "ThachAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thachai/multi-platform-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/thachai/multi-platform-assistant/issues"
  },
  "homepage": "https://github.com/thachai/multi-platform-assistant#readme"
}